<body>


    <script>
        let bases = parseInt(prompt("ingresa la base"));

        let altura = parseInt(prompt("ingresa la altura"));
        let area;
        area = bases * altura;
        document.write("el area de el rectangulo es:" + area);
    </script>
</body>
